	"tokenColors": [
		// red
		{
			"name": "Invalid - Illegal",
			"scope": [
				"invalid.illegal",
				"entity.name.exception"
			],
			"settings": {
				"foreground": "#a60000"
			}
		},
		// red-faint
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"comment.block.preprocessor"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7f0000"
			}
		},
		// green
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#006800"
			}
		},
		// yellow
		{
			"name": "Strings: Symbols",
			"scope": [
				"constant.other.symbol",
				
			],
			"settings": {
				"foreground": "#6f5500"
			}
		},
		// yellow-warmer
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#884900"
			}
		},
		// blue
		{
			"name": "String: Doublequotes",
			"scope": [
				"string.regexp",
				"entity.name.section",
			],
			"settings": {
				"foreground": "#0031a9",
			}
		},
		// blue-warmer
		{
			"name": "Strings: Escape Sequences",
			"scope": [
				"constant.character.escape",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"string.quoted"
			],
			"settings": {
				"foreground": "#3548cf"
			}
		},
		// blue-cooler
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language",
			],
			"settings": {
				"foreground": "#0000b0"
			}
		},
		// magenta
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#721045"
			}
		},
		// magenta-warmer
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#8f0075"
			}
		},
		// magenta-cooler
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#531ab6"
			}
		},
		// cyan
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#005e8b"
			}
		},
		// cyan-cooler
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#005f5f"
			}
		},
		// fg-main
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation",
			],
			"settings": {
				"foreground": "#000000"
			}
		},
	]
}
