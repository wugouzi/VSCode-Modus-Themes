	"tokenColors": [
		// red
		{
			"name": "Invalid - Illegal",
			"scope": [
				"invalid.illegal",
				"entity.name.exception"
			],
			"settings": {
				"foreground": "#ff5f59"
			}
		},
		// red-faint
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"comment.block.preprocessor"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff9f80"
			}
		},
		// green
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#44bc44"
			}
		},
		// yellow
		{
			"name": "Strings: Symbols",
			"scope": [
				"constant.other.symbol",
				"punctuation",
			],
			"settings": {
				"foreground": "#d0bc00"
			}
		},
		// yellow-warmer
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#fec43f"
			}
		},
		// blue
		{
			"name": "String: Doublequotes",
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"string.quoted.double",
				"string.regexp",
				"entity.name.section",
			],
			"settings": {
				"foreground": "#2fafff",
			}
		},
		// blue-warmer
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#79a8ff"
			}
		},
		// blue-cooler
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#00bcff"
			}
		},
		// magenta
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#feacd0"
			}
		},
		// magenta-warmer
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f78fe7"
			}
		},
		// magenta-cooler
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"entity.name.tag",
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#b6a0ff"
			}
		},
		// cyan
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#00d3d0"
			}
		},
		// cyan-cooler
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#6ae4b9"
			}
		},
		// fg-main
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#ffffff"
			}
		},
	]
}